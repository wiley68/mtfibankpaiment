<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
<policies>
    <policy id="font-src">
        <values>
            <value id="google-fonts" type="host">fonts.googleapis.com</value>
            <value id="google-fonts-static" type="host">fonts.gstatic.com</value>
        </values>
    </policy>
    <policy id="style-src">
        <values>
            <value id="google-fonts-api" type="host">fonts.googleapis.com</value>
        </values>
    </policy>
    <policy id="img-src">
        <values>
            <value id="fibank-images" type="host">fibank.avalon-bg.eu</value>
        </values>
    </policy>
    <policy id="connect-src">
        <values>
            <value id="fibank-connect" type="host">fibank.avalon-bg.eu</value>
        </values>
    </policy>
</policies>
</csp_whitelist>
